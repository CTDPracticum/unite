=begin%>
 First page rooted to users#new with create user and login forms 
<%
=end%>

<div class= 'row'>
  <div class= 'col'>
    <h1>Sign Up</h1>
    <%= form_for @user do |f|%>
      <%= f.label :name%><br>
      <%= f.text_field :name%><br>
      <%= f.label :email%><br>
      <%= f.email_field :email%><br> 
      <%= f.label :password%><br>
      <%= f.password_field :password%><br>
      <%= f.submit %>
    <% end %>
  </div>
 
